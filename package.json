{
  "name": "d2c-gameserver",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "docker": "docker build . -t dota2classic/matchmaking:gameserver && docker push dota2classic/matchmaking:gameserver",
    "docker:local": "docker build . -t dota2classic/matchmaking:gameserver",
    "nodemon": "nodemon scrapper.js",
    "migration:generate": "npx ts-node -P ./tsconfig.json -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate -d ./src/config/typeorm.config.ts",
    "migration:create": "npx ts-node -P ./tsconfig.json -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:create",
    "migration:run": "npx ts-node -P ./tsconfig.json -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d ./src/config/typeorm.config.ts",
    "migration:run-fake": "npx ts-node -P ./tsconfig.json -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run --fake -d ./src/config/typeorm.config.ts",
    "migration:revert": "npx ts-node -P ./tsconfig.json -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert -d ./src/config/typeorm.config.ts"
  },
  "dependencies": {
    "@nestjs/cache-manager": "^2.3.0",
    "@nestjs/common": "^10.4.10",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.10",
    "@nestjs/cqrs": "^10.2.8",
    "@nestjs/microservices": "^10.4.10",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^10.4.10",
    "@nestjs/schedule": "^4.1.1",
    "@nestjs/swagger": "^8.0.7",
    "@nestjs/typeorm": "^10.0.2",
    "@opentelemetry/auto-instrumentations-node": "^0.53.0",
    "@opentelemetry/exporter-trace-otlp-grpc": "^0.55.0",
    "@opentelemetry/instrumentation-express": "^0.45.0",
    "@opentelemetry/instrumentation-http": "^0.55.0",
    "@opentelemetry/instrumentation-nestjs-core": "^0.42.0",
    "@opentelemetry/instrumentation-pg": "^0.48.0",
    "@opentelemetry/resources": "^1.28.0",
    "@opentelemetry/sdk-node": "^0.55.0",
    "@opentelemetry/semantic-conventions": "^1.28.0",
    "@willsoto/nestjs-prometheus": "^6.0.2",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.5",
    "cache-manager": "^5.7.6",
    "cache-manager-redis-yet": "^5.1.4",
    "cheerio": "^1.0.0",
    "dotenv": "^16.4.5",
    "fluent-logger": "^3.4.1",
    "ioredis": "^5.4.1",
    "js-yaml": "^4.1.0",
    "passport-http": "^0.3.0",
    "pg": "^8.13.1",
    "pg-hstore": "^2.3.4",
    "prom-client": "^15.1.3",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.1",
    "rxjs-async-map": "^0.3.0-preview.0",
    "sequelize": "^6.37.5",
    "sqlite3": "^5.1.7",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.20",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.8",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.4.10",
    "@swc/cli": "^0.5.1",
    "@swc/core": "^1.9.3",
    "@testcontainers/postgresql": "^10.15.0",
    "@testcontainers/rabbitmq": "^10.18.0",
    "@testcontainers/redis": "^10.18.0",
    "@types/express": "^5.0.0",
    "@types/jest": "29.5.14",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.10.0",
    "@types/passport-http": "^0.3.11",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "8.16.0",
    "@typescript-eslint/parser": "8.16.0",
    "eslint": "9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "jest": "29.7.0",
    "nodemon": "^3.1.7",
    "prettier": "^3.4.1",
    "supertest": "^7.0.0",
    "testcontainers": "^10.15.0",
    "ts-jest": "29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleDirectories": [
      "<rootDir>",
      "node_modules"
    ],
    "setupFilesAfterEnv": [
      "../jest.ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
